{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "no-var-requires": false
  },
  "linterOptions": {
    "exclude": [
      "src/generated/*.ts"
    ]
  }
}
